{
  "_version": "1.49.0",
  "sap.app": {
    "id": "com.company.adminconfig",
    "type": "application",
    "title": "Admin Configuration",
    "description": "Manage validation rules, code lists, and system configuration",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      "mainService": {
        "uri": "/admin/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.120.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ui.comp": {},
        "sap.ui.layout": {},
        "sap.ushell": {}
      }
    },
    "rootView": {
      "viewName": "com.company.adminconfig.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.company.adminconfig.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "com.company.adminconfig.view",
        "controlId": "splitApp",
        "controlAggregation": "detailPages",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "master",
          "target": ["master", "detail"]
        },
        {
          "pattern": "detail/{entitySet}",
          "name": "detail",
          "target": ["master", "detailLoaded"]
        }
      ],
      "targets": {
        "master": {
          "viewName": "Master",
          "viewLevel": 1,
          "controlAggregation": "masterPages"
        },
        "detail": {
          "viewName": "Detail",
          "viewLevel": 2
        },
        "detailLoaded": {
          "viewName": "Detail",
          "viewLevel": 2
        }
      }
    },
    "extends": {
      "extensions": {}
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
