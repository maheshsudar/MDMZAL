{
    "_version": "1.49.0",
    "sap.app": {
        "id": "com.company.pirequests",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:lrop",
            "version": "1.8.0"
        },
        "dataSources": {
            "mainService": {
                "uri": "/pi/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "4.0"
                }
            },
            "adminService": {
                "uri": "/admin/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://factory",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "extends": {
            "extensions": {
                "sap.ui.controllerExtensions": {
                    "sap.fe.templates.ListReport.ListReportController": {
                        "controllerName": "com.company.pirequests.ext.controller.ListReportExt"
                    },
                    "sap.fe.templates.ObjectPage.ObjectPageController": {
                        "controllerName": "com.company.pirequests.ext.controller.ObjectPageExt",
                        "actions": {
                            "PIService.checkDuplicates": {
                                "press": "onCheckDuplicates"
                            }
                        }
                    }
                }
            }
        },
        "dependencies": {
            "minUI5Version": "1.120.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ushell": {},
                "sap.fe.templates": {},
                "sap.f": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.suite.ui.commons": {},
                "sap.suite.ui.microchart": {},
                "sap.ui.layout": {},
                "sap.ui.unified": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.company.pirequests.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": false,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            },
            "admin": {
                "dataSource": "adminService",
                "preload": false,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": false
                }
            }
        },
        "routing": {
            "config": {
                "statePreservation": false
            },
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "PIRequestsList",
                    "target": "PIRequestsList"
                },
                {
                    "pattern": "PIRequests({key}):?query:",
                    "name": "PIRequestsObjectPage",
                    "target": "PIRequestsObjectPage"
                }
            ],
            "targets": {
                "PIRequestsList": {
                    "type": "Component",
                    "id": "PIRequestsList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "contextPath": "/PIRequests",
                            "variantManagement": "Page",
                            "navigation": {
                                "PIRequests": {
                                    "detail": {
                                        "route": "PIRequestsObjectPage"
                                    }
                                }
                            }
                        }
                    }
                },
                "PIRequestsObjectPage": {
                    "type": "Component",
                    "id": "PIRequestsObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "editableHeaderContent": false,
                            "contextPath": "/PIRequests"
                        }
                    }
                }
            }
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "com.company.pirequests"
    }
}
