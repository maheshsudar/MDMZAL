{
  "_version": "1.49.0",
  "sap.app": {
    "id": "com.company.satelliteack",
    "type": "application",
    "title": "Satellite System Acknowledgements",
    "description": "Acknowledge shared BP changes from other satellite systems",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      "mainService": {
        "uri": "/satellite-ack/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.120.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ushell": {},
        "sap.fe.templates": {},
        "sap.f": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.suite.ui.commons": {},
        "sap.suite.ui.microchart": {},
        "sap.ui.layout": {},
        "sap.ui.unified": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.company.satelliteack.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "routing": {
      "config": {
        "statePreservation": false
      },
      "routes": [
        {
          "pattern": ":?query:",
          "name": "AcknowledgementsList",
          "target": "AcknowledgementsList"
        },
        {
          "pattern": "AllAcknowledgements({key}):?query:",
          "name": "AcknowledgementsObjectPage",
          "target": "AcknowledgementsObjectPage"
        }
      ],
      "targets": {
        "AcknowledgementsList": {
          "type": "Component",
          "id": "AcknowledgementsList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "entitySet": "AllAcknowledgements",
              "variantManagement": "Page",
              "navigation": {
                "AllAcknowledgements": {
                  "detail": {
                    "route": "AcknowledgementsObjectPage"
                  }
                }
              }
            }
          }
        },
        "AcknowledgementsObjectPage": {
          "type": "Component",
          "id": "AcknowledgementsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "entitySet": "AllAcknowledgements",
              "navigation": {}
            }
          }
        }
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
