{
  "_version": "1.49.0",
  "sap.app": {
    "id": "com.company.mdmapproval",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.8.0"
    },
    "dataSources": {
      "mainService": {
        "uri": "/mdm/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      },
      "adminService": {
        "uri": "/admin/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://business-partner",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "extends": {
      "extensions": {
        "sap.ui.controllerExtensions": {
          "sap.fe.templates.ListReport.ListReportController": {
            "controllerName": "com.company.mdmapproval.ext.controller.ListReportExt"
          },
          "sap.fe.templates.ObjectPage.ObjectPageController": {
            "controllerName": "com.company.mdmapproval.ext.controller.ObjectPageExt"
          }
        }
      }
    },
    "dependencies": {
      "minUI5Version": "1.120.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ushell": {},
        "sap.fe.templates": {},
        "sap.f": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.company.mdmapproval.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "admin": {
        "dataSource": "adminService",
        "preload": false,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": false
        }
      }
    },
    "routing": {
      "config": {
        "statePreservation": false
      },
      "routes": [
        {
          "pattern": ":?query:",
          "name": "BusinessPartnerRequestsList",
          "target": "BusinessPartnerRequestsList"
        },
        {
          "pattern": "MDMApprovalRequests({key}):?query:",
          "name": "BusinessPartnerRequestsObjectPage",
          "target": "BusinessPartnerRequestsObjectPage"
        }
      ],
      "targets": {
        "BusinessPartnerRequestsList": {
          "type": "Component",
          "id": "BusinessPartnerRequestsList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/MDMApprovalRequests",
              "variantManagement": "Page",
              "initialLoad": true,
              "navigation": {
                "MDMApprovalRequests": {
                  "detail": {
                    "route": "BusinessPartnerRequestsObjectPage"
                  }
                }
              }
            }
          }
        },
        "BusinessPartnerRequestsObjectPage": {
          "type": "Component",
          "id": "BusinessPartnerRequestsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": true,
              "contextPath": "/MDMApprovalRequests",
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.Identification": {
                  "actions": {
                    "DataFieldForAction::StandardAction::Edit": {
                      "visible": true
                    },
                    "DataFieldForAction::StandardAction::Delete": {
                      "visible": false
                    },
                    "DataFieldForAction::MDMService.performAEBCheck": {
                      "press": ".extension.com.company.mdmapproval.ext.controller.ObjectPageExt.onPerformAEBCheck",
                      "requiresSelection": false
                    },
                    "DataFieldForAction::MDMService.performVIESCheck": {
                      "press": ".extension.com.company.mdmapproval.ext.controller.ObjectPageExt.onPerformVIESCheck",
                      "requiresSelection": false
                    },
                    "DataFieldForAction::MDMService.checkDuplicates": {
                      "press": ".extension.com.company.mdmapproval.ext.controller.ObjectPageExt.onCheckDuplicates",
                      "requiresSelection": false
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "com.company.mdmapproval"
  }
}